extends ./default

block head
	title title
block content
	p#_csrf(hidden, name='_csrf', value=csrfToken)
	p
		| Welcome #{user.username} 
		a(href="/signout", title="sign out") sign out
block js
	script.
		var socket = io(location.origin + '?_csrf=' + encodeURIComponent($("#_csrf").attr("value")));